{
   "compilerOptions": {
      "lib": [
         "es6"
      ],
      "noEmit": true,
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./dist",
      "esModuleInterop":true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "strict": false,         
      "skipLibCheck": true,
      "rootDir": "./",
      "strictPropertyInitialization": false,
      "forceConsistentCasingInFileNames": true
   }
}